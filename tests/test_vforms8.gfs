import -retain ../estonian/MorphoEst.gf

--Regular verbs, test whether vForms8 gives the same results as vForms4
cc -table -unqual vforms82VEst ( vForms8 "lugema" "lugeda" "loeb" "lugege" "loetakse" "luges" "loetud" "lugev" )
cc -table -unqual vforms82VEst ( vForms8  "vestlema" "vestelda" "vestleb" "vestelge" "vesteldakse" "vestles" "vesteldud" "vestlev" )
cc -table -unqual vforms82VEst (vForms8 "kinkima" "kinkida" "kingib" "kinkige" "kingitakse" "kinkis" "kingitud" "kinkiv")

--Irregular verbs: the 27 that can't be formed with vForms4

--olema, minema
cc -table -unqual vforms82VEst ( vForms8  "olema" "olla" "on" "olge" "ollakse" "oli" "oldud" "olev" ) 
cc -table -unqual vforms82VEst ( vForms8  "minema" "minna" "läheb" "minge" "minnakse" "läks" "mindud" "minev" )  

--tegema, nägema
cc -table -unqual vforms82VEst ( vForms8  "tegema" "teha" "teeb" "tehke" "tehakse" "tegi" "tehtud" "tegev" ) 
cc -table -unqual vforms82VEst ( vForms8  "nägema" "näha" "nääb" "nähke" "nähakse" "nägi" "nähtud" "nägev" ) 

--laskma 
